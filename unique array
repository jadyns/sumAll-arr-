function uniteUnique(arr) {
  var array = [];
  int check = 0;
  for (i=0; i < arguments.length; i++) {
    for (int j = 0; j<array.length() ; j++) {
      if (array[j] == arguments[i]){
        check = 1;
      }
    }
    if(check ==0 ){
      array.push(arguments[i]);
    }
    check = 0;
  }
 var unique = [];
 unique = array.sort();
 return unique;
}
